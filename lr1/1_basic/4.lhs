  Задание 1.4

  xxx s = s>='0' && s<='7';
  -- xxx проверяет, принадлежит ли символ s
  -- диапазону ['0'..'7']

  yyy s = s>='0' && s<='9';
  -- yyy проверяет, принадлежит ли символ s
  -- диапазону ['0'..'9']

  zzz s = s>='A' && s<='F' || s>='a' && s<='f';
  -- zzz проверяет, принадлежит ли символ s
  -- хотя бы одному из диапазонов
  -- ['A'..'F'] и ['a'..'f']
